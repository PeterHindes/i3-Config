# switch to workspace

bindsym $mod+1 mode "mon1"
bindsym $mod+2 mode "mon2"
bindsym $mod+3 mode "mon3"
bindsym $mod+4 mode "mon4"
bindsym $mod+5 mode "mon5"
bindsym $mod+6 mode "mon6"
bindsym $mod+7 mode "mon7"

mode "mon1" {
  bindsym $mod+q workspace $ws1a; mode "default"
  bindsym $mod+w workspace $ws1b; mode "default"
  bindsym $mod+e workspace $ws1c; mode "default"
  bindsym $mod+r workspace $ws1d; mode "default"
  bindsym $mod+t workspace $ws1e; mode "default"
  bindsym $mod+y workspace $ws1f; mode "default"
  bindsym $mod+u workspace $ws1g; mode "default"
  bindsym $mod+i workspace $ws1h; mode "default"
  bindsym $mod+o workspace $ws1i; mode "default"
  bindsym $mod+p workspace $ws1j; mode "default"

  bindsym $mod+Shift+q move container to workspace $ws1a; mode "default"
  bindsym $mod+Shift+w move container to workspace $ws1b; mode "default"
  bindsym $mod+Shift+e move container to workspace $ws1c; mode "default"
  bindsym $mod+Shift+r move container to workspace $ws1d; mode "default"
  bindsym $mod+Shift+t move container to workspace $ws1e; mode "default"
  bindsym $mod+Shift+y move container to workspace $ws1f; mode "default"
  bindsym $mod+Shift+u move container to workspace $ws1g; mode "default"
  bindsym $mod+Shift+i move container to workspace $ws1h; mode "default"
  bindsym $mod+Shift+o move container to workspace $ws1i; mode "default"
  bindsym $mod+Shift+p move container to workspace $ws1j; mode "default"

  bindsym $modalt+Shift+q move container to workspace $ws1a; workspace $ws1a; mode "default"
  bindsym $modalt+Shift+w move container to workspace $ws1b; workspace $ws1b; mode "default"
  bindsym $modalt+Shift+e move container to workspace $ws1c; workspace $ws1c; mode "default"
  bindsym $modalt+Shift+r move container to workspace $ws1d; workspace $ws1d; mode "default"
  bindsym $modalt+Shift+t move container to workspace $ws1e; workspace $ws1e; mode "default"
  bindsym $modalt+Shift+y move container to workspace $ws1f; workspace $ws1f; mode "default"
  bindsym $modalt+Shift+u move container to workspace $ws1g; workspace $ws1g; mode "default"
  bindsym $modalt+Shift+i move container to workspace $ws1h; workspace $ws1h; mode "default"
  bindsym $modalt+Shift+o move container to workspace $ws1i; workspace $ws1i; mode "default"
  bindsym $modalt+Shift+p move container to workspace $ws1j; workspace $ws1j; mode "default"

  bindsym Escape mode "default"
}

