  bindsym $mod+u workspace $ws6g; mode "default"
  bindsym $mod+i workspace $ws6h; mode "default"
  bindsym $mod+o workspace $ws6i; mode "default"
  bindsym $mod+p workspace $ws6j; mode "default"

  bindsym $mod+Shift+q move container to workspace $ws6a; mode "default"
  bindsym $mod+Shift+w move container to workspace $ws6b; mode "default"
  bindsym $mod+Shift+e move container to workspace $ws6c; mode "default"
  bindsym $mod+Shift+r move container to workspace $ws6d; mode "default"
  bindsym $mod+Shift+t move container to workspace $ws6e; mode "default"
  bindsym $mod+Shift+y move container to workspace $ws6f; mode "default"
  bindsym $mod+Shift+u move container to workspace $ws6g; mode "default"
  bindsym $mod+Shift+i move container to workspace $ws6h; mode "default"
  bindsym $mod+Shift+o move container to workspace $ws6i; mode "default"
  bindsym $mod+Shift+p move container to workspace $ws6j; mode "default"

  bindsym $modalt+Shift+q move container to workspace $ws6a; workspace $ws6a; mode "default"
  bindsym $modalt+Shift+w move container to workspace $ws6b; workspace $ws6b; mode "default"
  bindsym $modalt+Shift+e move container to workspace $ws6c; workspace $ws6c; mode "default"
  bindsym $modalt+Shift+r move container to workspace $ws6d; workspace $ws6d; mode "default"
  bindsym $modalt+Shift+t move container to workspace $ws6e; workspace $ws6e; mode "default"
  bindsym $modalt+Shift+y move container to workspace $ws6f; workspace $ws6f; mode "default"
  bindsym $modalt+Shift+u move container to workspace $ws6g; workspace $ws6g; mode "default"
  bindsym $modalt+Shift+i move container to workspace $ws6h; workspace $ws6h; mode "default"
  bindsym $modalt+Shift+o move container to workspace $ws6i; workspace $ws6i; mode "default"
  bindsym $modalt+Shift+p move container to workspace $ws6j; workspace $ws6j; mode "default"

  bindsym Escape mode "default"
}

